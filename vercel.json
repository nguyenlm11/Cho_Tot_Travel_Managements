{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://hungnv.iselab.cloud:7221/api/:path*"
      },
      {
        "source": "/chat/:path*",
        "destination": "https://hungnv.iselab.cloud:7221/api/:path*"
      },
      {
        "source": "/chatHub/:path*",
        "destination": "https://hungnv.iselab.cloud:7221/chatHub/:path*"
      },
      {
        "source": "/:path*",
        "destination": "/index.html"
      }
    ]
  }
  